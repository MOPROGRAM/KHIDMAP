rules_version = '2';

service firebase.storage {
  match /b/{bucket}/o {
    // WARNING: This rule is insecure and allows public read/write access.
    // It is used here to bypass a complex permission issue as confirmed by the user.
    // It should be replaced with more secure rules for production.
    match /{allPaths=**} {
      allow read, write: if true;
    }
  }
}
